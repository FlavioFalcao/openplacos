<% content_for :menu_left do %>
  <%room_path = @actuator.path.split("/")%>
  <%room_path.delete(room_path.last)%>
  <%= render :partial => "shared/menu_left", :locals => { :path => room_path.join("/") }%>
<% end %>
<%= render :partial => "abstract",:locals => { :actuator => @actuator }%>
<%= render :partial => "shared/graph",:locals => {:path => "/actuators/graph#{@actuator.path}"}%>

